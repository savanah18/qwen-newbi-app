{
  "name": "triton-ai-chat",
  "displayName": "Triton AI Chat Assistant",
  "description": "Generic AI chat assistant powered by Triton Inference Server with batch inference, embedding extraction, and multimodal support.",
  "version": "0.2.0",
  "publisher": "your-publisher",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onCommand:tritonAI.start",
    "onCommand:tritonAI.openChat"
  ],
  "contributes": {
    "commands": [
      {
        "command": "tritonAI.start",
        "title": "Triton AI: Start"
      },
      {
        "command": "tritonAI.openChat",
        "title": "Triton AI: Open Chat Assistant"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "echo 'No lint configured'",
    "package": "vsce package || echo 'Install vsce to package'"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.87.0",
    "typescript": "^5.3.3"
  }
}
